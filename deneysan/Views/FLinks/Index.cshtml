@model IEnumerable<deneysan_DAL.Entities.ImportantLinks>
@{
    Layout = "~/Views/Shared/frontlayout.cshtml";
}
<div class="main_content mainpage pageheight" style="">
    <div class="wrapper mainpage grid_12">
        <div class="content_page page">
            <h2>@LinksRess.LinksStrings.importantlinks</h2>
            <hr class="wide" />
            <div class="ulListe">
                <ul id="newslist">
                    @foreach(var links in Model)
                    {
                    <li>
                        <a href="@links.LinkUrl">
                            <span class="Left">@links.LinkName</span>
                        </a>
                    </li>
                    }
                </ul>
            </div>
        </div>
    </div>
</div>
