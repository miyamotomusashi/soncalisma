@model IEnumerable<deneysan_DAL.Entities.News>
@{
    Layout = "~/Views/Shared/frontlayout.cshtml";
}
<div class="main_content mainpage pageheight" style="">
    <div class="wrapper mainpage grid_12">
        <div class="content_page page">
            <h2>@NewsRess.NewsStrings.deneysannews</h2>
            <hr class="wide" />
            <div class="ulListe">
                <ul id="newslist">
                    @foreach(var news in Model)
                    {
                    <li>
                        <a href="/@SharedRess.SharedStrings.menu_culture/@SharedRess.SharedStrings.menu_news/@news.PageSlug/@news.NewsId">
                            <span class="Left">@news.Header</span>
                            <span class="Right">@news.TimeCreated.Value.ToString("dd.MM.yyyy")</span>
                        </a>
                    </li>
                    }
                </ul>
            </div>
        </div>
    </div>
</div>
